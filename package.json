    "test": "echo \"Error: no test specified\" && exit 1",
    "sass-compile": "npm run compile-styles-sass && css-minify",
    "compile-styles-sass": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 styles/main.scss styles/main.css",
    "css-minify": "cleancss --level 1 --format breaksWith=lf --source-map --source-map-inline-sources --output styles/main.min.css styles/main.css"
  },
